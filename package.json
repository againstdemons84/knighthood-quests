{
  "name": "workout-svg-generator",
  "version": "1.0.0",
  "description": "A React application that generates SVG visualizations based on workout data from an API.",
  "main": "src/index.ts",
  "homepage": "https://againstdemons84.github.io/knighthood-quests/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:desktop": "playwright test --project desktop-chromium desktop-firefox desktop-webkit",
    "test:e2e:mobile": "playwright test --project mobile-chromium mobile-webkit",
    "test:e2e:cross-platform": "playwright test cross-platform.spec.ts",
    "test:all": "npm run test -- --watchAll=false && npm run test:e2e",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "fetch-workouts": "node src/fetch-workouts.js"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "gh-pages": "^6.3.0",
    "jest-environment-jsdom": "^30.2.0",
    "webpack-cli": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/workout-svg-generator.git"
  },
  "keywords": [
    "react",
    "svg",
    "workout",
    "visualization"
  ],
  "author": "Your Name",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
